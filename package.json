    "gatsby-plugin-sass": "^4.7.1",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-sitemap": "^4.3.1",
    "gatsby-remark-autolink-headers": "^4.4.1",
    "gatsby-remark-copy-linked-files": "^4.4.1",
    "gatsby-remark-images": "^5.4.1",
    "gatsby-remark-mermaid": "^2.1.0",
    "gatsby-remark-prismjs": "^5.4.1",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-transformer-sharp": "^3.7.1",
    "littlefoot": "^3.2.5",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.24.0",
    "puppeteer": "^5.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "clipboardy": "^2.3.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mdx": "^1.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "inquirer": "^7.3.3",
    "mkdirp": "^1.0.4",
    "prettier": "^2.1.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "yarn develop",
    "develop": "gatsby develop -p 1234",
    "format": "prettier --write \"**/*.{js,jsx,json,md,mdx}\"",
    "start": "yarn develop",
    "serve": "yarn prod",
    "prod": "gatsby build && gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint src posts",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "post": "./bin/new-post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "standard",
      "standard-react",
      "plugin:mdx/recommended",
      "prettier"
    ],
    "rules": {
      "react/prop-types": 0
    },
    "overrides": [
      {
        "files": [
          "*.mdx"
        ],
        "rules": {
          "react/jsx-indent": 0
        },
        "globals": {
          "YoutubeEmbed": true,
          "BlockquoteTitle": true
        }
      }
    ]
  },
  "prettier": {}
}
